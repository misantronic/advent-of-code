{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "rootDir": "."
    },
    "include": ["./**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}
